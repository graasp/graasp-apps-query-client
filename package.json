    "globals": "15.9.0",
    "happy-dom": "14.12.3",
    "husky": "9.1.1",
    "nock": "13.5.4",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-test-renderer": "18.3.1",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "vite": "5.3.4",
