{
  "name": "@graasp/apps-query-client",
  "version": "2.0.4",
  "description": "Query client repository for Graasp apps",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:graasp/graasp-apps-query-client.git",
  "author": "Graasp",
  "contributors": [
    "Kim Lan Phan Hoang",
    "Basile Spaenlehauer",
    "Jérémy La Scala"
  ],
  "workspaces": [
    "example"
  ],
  "license": "AGPL3.0-only",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "gh-pages -d example/build",
    "commit-msg": "commitlint --edit $1",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "check": "yarn lint && yarn prettier:check && yarn type-check",
    "pre-commit": "yarn prettier:check && yarn lint",
    "predeploy": "cd example && yarn install && yarn run build",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "post-commit": "git status",
    "start:example": "yarn workspace graasp-query-client-example start",
    "test": "jest --forceExit --coverage --verbose --silent",
    "test:watch": "yarn test --watchAll",
    "test:ci": "yarn && cd example && yarn && cd .. && yarn test"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@graasp/sdk": "1.10.1",
    "@mui/icons-material": "5.14.7",
    "@mui/material": "5.14.7",
    "axios": "0.27.2",
    "dexie": "3.2.4",
    "http-status-codes": "2.2.0",
    "immutable": "4.3.4",
    "miragejs": "0.1.47",
    "msw": "1.3.2",
    "qs": "6.11.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@tanstack/react-query": "4.33.0",
    "@tanstack/react-query-devtools": "4.33.0",
    "@testing-library/react": "14.0.0",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/jest": "29.5.4",
    "@types/node": "20.5.7",
    "@types/qs": "6.9.8",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "9.0.3",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "@vitejs/plugin-react": "4.0.4",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-environment-jsdom": "29.6.4",
    "jest-immutable-matchers": "3.0.0",
    "nock": "13.3.3",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-checker": "0.6.2",
    "vite-plugin-dts": "3.5.3",
    "vitest": "0.34.6"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-query-devtools": "^4.28.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "files": [
    "dist"
  ],
  "packageManager": "yarn@3.6.3"
}
