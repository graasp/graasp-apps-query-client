{
  "name": "@graasp/apps-query-client",
  "version": "0.2.0",
  "description": "Query client repository for Graasp apps",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "types": "dist/src/index.d.ts",
  "repository": "git@github.com:graasp/graasp-apps-query-client.git",
  "author": "Graasp Association",
  "workspaces": [
    "example"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "0.26.0",
    "http-status-codes": "2.1.4",
    "immutable": "4.0.0",
    "miragejs": "0.1.43",
    "qs": "6.10.3",
    "react-query": "3.31.0",
    "standard-version": "9.5.0",
    "tsc": "2.0.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.1",
    "@types/node": "17.0.10",
    "@types/qs": "6.9.7",
    "@types/react": "17.0.38",
    "@types/react-query": "1.2.9",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "jest": "27.3.0",
    "microbundle-crl": "0.13.11",
    "nock": "13.2.1",
    "prettier": "2.5.1",
    "react-test-renderer": "17.0.2",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "commit-msg": "commitlint --edit $1",
    "deploy": "gh-pages -d example/build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint",
    "predeploy": "cd example && yarn install && yarn run build",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "post-commit": "git status",
    "release": "git fetch --tags && git add CHANGELOG.md && standard-version -a",
    "release:first": "git fetch --tags && git add CHANGELOG.md && standard-version -a --first-release",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "start:example": "yarn workspace graasp-query-client-example start",
    "test": "jest src/hooks/appSetting.test.ts --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "test:ci": "yarn && cd example && yarn && cd .. && yarn test"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "localDependencies": {
    "react": "./example/node_modules/react",
    "react-dom": "./example/node_modules/react-dom"
  },
  "files": [
    "dist"
  ],
  "packageManager": "yarn@3.2.0-rc.10"
}
