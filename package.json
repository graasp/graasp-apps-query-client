{
  "name": "@graasp/apps-query-client",
  "version": "0.2.0",
  "description": "Query client repository for Graasp apps",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:graasp/graasp-apps-query-client.git",
  "author": "Graasp Association",
  "contributors": [
    "Kim Lan Phan Hoang",
    "Basile Spaenlehauer"
  ],
  "workspaces": [
    "example"
  ],
  "license": "MIT",
  "dependencies": {
    "@graasp/utils": "github:graasp/graasp-utils",
    "axios": "0.27.2",
    "http-status-codes": "2.2.0",
    "immutable": "4.1.0",
    "miragejs": "0.1.45",
    "qs": "6.10.5",
    "react-query": "4.0.0-beta.23",
    "standard-version": "9.5.0",
    "tsc": "2.0.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@testing-library/react-hooks": "8.0.0",
    "@types/jest": "28.1.2",
    "@types/node": "17.0.45",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.14",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "microbundle": "0.15.0",
    "nock": "13.2.7",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "scripts": {
    "build": "microbundle --no-compress --format modern,cjs --jsx React.createElement",
    "commit-msg": "commitlint --edit $1",
    "deploy": "gh-pages -d example/build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint",
    "predeploy": "cd example && yarn install && yarn run build",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "post-commit": "git status",
    "release": "git fetch --tags && git add CHANGELOG.md && standard-version -a",
    "release:first": "git fetch --tags && git add CHANGELOG.md && standard-version -a --first-release",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "start:example": "yarn workspace graasp-query-client-example start",
    "test": "jest src/hooks/appSetting.test.ts --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "test:ci": "yarn && cd example && yarn && cd .. && yarn test"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "localDependencies": {
    "react": "./example/node_modules/react",
    "react-dom": "./example/node_modules/react-dom"
  },
  "files": [
    "dist"
  ],
  "packageManager": "yarn@3.2.1"
}
