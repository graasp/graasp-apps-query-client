{
  "name": "@graasp/graasp-apps-query-client",
  "version": "0.1.0",
  "description": "Query client repository for Graasp apps",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "repository": "git@github.com:graasp/graasp-apps-query-client.git",
  "author": "Graasp Association",
  "license": "MIT",
  "dependencies": {
    "axios": "0.21.4",
    "http-status-codes": "2.1.4",
    "immutable": "4.0.0-rc.12",
    "js-cookie": "3.0.0",
    "qs": "6.10.1",
    "react-query": "3.31.0",
    "tsc": "^2.0.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "27.0.1",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "7.0.4",
    "jest": "27.3.0",
    "microbundle-crl": "0.13.11",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.2",
    "ts-jest": "27.0.7",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "commit-msg": "commitlint --edit $1",
    "deploy": "gh-pages -d example/build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "pre-commit": "pretty-quick --staged && yarn lint",
    "predeploy": "cd example && yarn install && yarn run build",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "post-commit": "git status",
    "release": "standard-version",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "start:example": "yarn workspace graasp-query-client-example start",
    "test": "jest  --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "test:ci": "yarn && cd example && yarn && cd .. && yarn test"
  },
  "files": [
    "dist"
  ],
  "packageManager": "yarn@3.2.0-rc.10"
}
